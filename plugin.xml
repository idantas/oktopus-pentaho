<?xml version="1.0" encoding="UTF-8"?>
<plugin title="oktopus" loader="DEFAULT">
<overlays>
		<overlay id="startup.nzn.oktopus" resourcebundle="content/oktopus/resources/lang/messages">
			<menubar id="toolsmenu">
	        	<menuitem id="oktopus" label="${Launcher.nzn.oktopus}" command="mantleXulHandler.openUrl('${Launcher.nzn.oktopus}','${Launcher.nzn.oktopus}','plugin/oktopus/api/default')"/>
	      	</menubar>
	    </overlay>
	</overlays>
	<lifecycle-listener class="pt.webdetails.cpk.CpkLifecycleListener"/>
	<static-paths>
		<static-path url="/oktopus/static" localFolder="static"/>
		<static-path url="/oktopus/resources" localFolder="resources"/>
		<static-path url="/oktopus/ui" localFolder="ui/app"/>
		<static-path url="/oktopus/dashboards" localFolder="dashboards"/>
		<static-path url="/oktopus/backend" localFolder="backend"/>
	</static-paths>
	<content-types>
		<content-type>
			<title>Oktopus</title>
			<description></description>
			<company name="" url="" logo="@PLUGIN_COMPANY_LOGO@"></company>
		</content-type>
	</content-types>
	<external-resources>
		<file context="analyzer">content/oktopus/ui/app/analyzer-events.js</file>
	</external-resources>
	<content-generator
		id="oktopus"
		title="Oktopus"
		type="oktopus"
		class="pt.webdetails.cpk.CpkContentGenerator"/>
	<!-- Menu entry -->
	<menu-items>
		
	</menu-items>
</plugin>
